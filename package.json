{
  "name": "blog-by-nuxt",
  "version": "1.0.0",
  "description": "blog service created by nuxt.js",
  "main": "index.js",
  "scripts": {
    "dev": "nuxt",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"

    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": ["js", "json", "vue"],
    "collectCoverageFrom": ["**/*.{js,vue}"]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "useBuiltIns": false
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "keywords": [],
  "author": "kneegorilla",
  "license": "ISC",
  "dependencies": {
    "@nuxtjs/auth": "^4.5.3",
    "@nuxtjs/axios": "^5.3.6",
    "element-ui": "^2.5.4",
    "firebase": "^5.8.2",
    "nuxt": "^2.4.3",
    "universal-cookie": "^3.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.1.0",
    "babel-preset-vue-app": "^2.0.0",
    "jest": "^24.1.0",
    "lodash.clonedeep": "^4.5.0",
    "vue-jest": "^3.0.2"
  }
}
